{
  "name": "kysely-plugin-prefix",
  "version": "1.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "NODE_OPTIONS='--import=tsx/cjs' ava",
