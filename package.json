    "@types/better-sqlite3": "~7.6.7",
    "ava": "~5.3.1",
    "better-sqlite3": "~9.0.0",
    "husky": "~8.0.3",
    "tsx": "~3.14.0",
    "typescript": "~5.2.2"
  },
  "license": "MIT",
  "keywords": [
    "kysely",
    "plugin"
  ],
  "dependencies": {
    "kysely": "~0.26.3"
